# Production Environment Configuration
# This file is used as a template for production environment variables

# Supabase Configuration - Production
NEXT_PUBLIC_SUPABASE_URL=your_production_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_production_supabase_service_role_key_here

# Colombian Configuration
COLOMBIA_TIMEZONE=America/Bogota
COLOMBIA_CURRENCY=COP
COLOMBIA_PHONE_PREFIX=+57

# Environment Info
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=production
NEXT_PUBLIC_APP_VERSION=1.0.0

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://your-production-domain.com
API_TIMEOUT=20000

# Feature Flags
ENABLE_DEBUG_MODE=false
ENABLE_TEST_DATA=false

# Logging
LOG_LEVEL=error

# Security
NEXTAUTH_SECRET=your_production_nextauth_secret_here
NEXTAUTH_URL=https://your-production-domain.com

# Performance
NEXT_PUBLIC_ENABLE_ANALYTICS=true